<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/FrameLayoutRoot" android:layout_height="fill_parent" android:layout_width="fill_parent"><ImageButton android:id="@+id/ImageButtonCell" android:background="@drawable/backpack_cell" android:layout_height="fill_parent" android:layout_width="fill_parent"></ImageButton>
